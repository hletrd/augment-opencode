  "dependencies": {
    "@augmentcode/auggie-sdk": "^0.1.15",
    "@opencode-ai/plugin": ">=0.15.0"
  },
  "devDependencies": {
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
